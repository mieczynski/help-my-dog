{
  "plugins": ["@prettier/plugin-php"],
  "printWidth": 120,
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "phpVersion": "8.2",
  "braceStyle": "per-cs",
  "overrides": [
    {
      "files": "*.php",
      "options": {
        "printWidth": 120,
        "tabWidth": 4
      }
    }
  ]
}
