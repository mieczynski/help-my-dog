{
    "name": "help-my-dog",
    "type": "project",
    "license": "MIT",
    "description": "Help My Dog - Symfony + Postgres + Redis",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/framework-bundle": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/console": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/messenger": "^7.0",
        "symfony/validator": "^7.0",
        "symfony/cache": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/uid": "^7.0",
        "doctrine/orm": "^3.2",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "nelmio/cors-bundle": "^2.4",
        "predis/predis": "^2.2",
        "symfony/yaml": "^7.3.3",
        "symfony/flex": "^2.4.7",
        "friendsofsymfony/rest-bundle": "^3.5",
        "ext-bcmath": "*",
        "doctrine/doctrine-fixtures-bundle": "^4.1.0",
        "lexik/jwt-authentication-bundle": "^v3.1.1"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.60",
        "symfony/var-dumper": "^7.0",
        "symfony/phpunit-bridge": "^7.0",
        "phpunit/phpunit": "^10.5",
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.58",
        "phpstan/phpstan": "^1.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "fix": [
            "php-cs-fixer fix --allow-risky=yes"
        ],
        "analyse": [
            "phpstan analyse -l 8 src"
        ],
        "test": [
            "php -d memory_limit=1G vendor/bin/phpunit"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.*"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
